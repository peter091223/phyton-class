def flip(y):
	if y == 0:
		return y + 1
	elif y == 1:
		return y - 1

print(flip(0))
print()

def is_even(n):
	return n % 2 == 0
print(is_even(4))
print()

def pos_com(num):
	return num ** 2
print(pos_com(3))
print()

def And(a,b):
	return a and b
print(True, True)
print()

def sum_minimums(lst):
	return sum(min(x) for x in lst)

print(sum_minimums([[1, 2, 3, 4, 5],[5, 6, 7, 8, 9],[20, 21, 34, 56, 100]]))