def my_function1():
    print("hello from a function")
    
def my_function2():
    print("hello from a function")
    
    my_function2()
    
def my_function3(fname):
    print(fname + " hong")
    
my_function3("Emil")
my_function3("Tobias")
my_function3("linus")

def my_function(fname, lname):
    print(fname + " " + lname)
    
my_function("Emil" , "hong")

def my_function4(child3, child2, child1):
    print("the yongest child is " + child3)
    
my_function4(child1="Emil", child2= "Tobias", child3= "Linus")

#from here is "passing list as an argument"

def my_function(food):
    for x in food:
        print(x)
        
fruits = ["apple", "banana", "cherry"]

my_function(fruits)

def my_function5 (x):
    return 5 * x
    
print (my_function5 (3))