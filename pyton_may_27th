odd = [2, 4, 6, 8]
odd[0] = 1

odd[1:4] = [3, 5, 7]

print(odd)

odd = ['o', "hell", "peach"]

odd.append("abc")

print(odd)

odd.extend(["hello", "apple", "banana"])

print(odd)

odd = [9, 7, 6]

print(odd + [1, 3, 5])

even = ["poop"]

print(even * 10)

odd = [1, 9]
odd.insert(1, 3)

print(odd)

odd[2:2] = [5, 7]

print(odd)

my_list = ['p', 'r', 'o', 'b', 'l', 'e', 'm']

del my_list[2]

print(my_list)

del my_list[1:5]

print(my_list)

del my_list