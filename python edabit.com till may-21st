def max_num(n1, n2):
	lst = [n1, n2]
	return max(lst)
print(max_num(2, 1))
print()

def is_plural(word):
	return word[-1] == 's'

print(is_plural("changes"))
print()

def even_last(lst):
	return sum(lst[::2]) * lst[-1] if lst else 0
print(even_last([1, 3, 4, 2, 10]))
print()

def profitable_gamble(prob, prize, pay):
	return prob * prize > pay

print(profitable_gamble(3, 7, 9))
print()

def k_to_k(n, k):
	return n == k ** k
print(3, 5)
